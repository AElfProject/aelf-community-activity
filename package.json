  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-aelf-community-activity --port=7100",
    "stop": "egg-scripts stop --title=egg-server-aelf-community-activity",
    "watch:dev": "npm run clean && webpack --progress --colors --display-chunks --config ./build/webpack.dev.js -w  --devtool eval-source-map",
    "build:dev": "npm run clean && webpack --progress --colors --display-chunks --config ./build/webpack.dev.js  --devtool eval-source-map",
    "build": "npm run clean && webpack --progress --colors --display-chunks --config ./build/webpack.prod.js",
    "clean": "rimraf app/public/*",
    "dev": "egg-bin dev --port=7100",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "create-db": "cross-env NODE_ENV=production sequelize db:create",
    "create-db:dev": "cross-env NODE_ENV=development sequelize db:create",
    "generate": "cross-env NODE_ENV=production sequelize migration:generate",
    "generate:dev": "cross-env NODE_ENV=development sequelize migration:generate",
    "migrate": "cross-env NODE_ENV=production sequelize db:migrate",
    "migrate:dev": "cross-env NODE_ENV=development sequelize db:migrate",
    "undo-all": "cross-env NODE_ENV=production sequelize db:migrate:undo:all",
    "undo-all:dev": "cross-env NODE_ENV=development sequelize db:migrate:undo:all"
  },
  "ci": {
    "version": "10"
  },
